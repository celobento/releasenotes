<p id="notice"><%= notice %></p>

<div class="panel panel-default panel-table">
              <div class="panel-heading">
                <div class="row">
                  <div class="col col-xs-6">
                    <h3 class="panel-title">Tipos de Integração</h3>
                  </div>
                  <div class="col col-xs-6 text-right">

                    <%= link_to new_tipo_integracao_path, class:"btn btn-primary btn-sm active" do %>
                      <span class="glyphicon glyphicon-plus"></span> Novo
                    <% end %>
                  </div>
                </div>
              </div>
              <div class="panel-body table-responsive">
                <table class="table table-striped table-bordered table-list">
  <thead>
    <tr>
      <th><span class="glyphicon glyphicon-cog"></span></th>
      <th>Nome</th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_integracoes.each do |tipo_integracao| %>
      <tr>
                        <td align="center">
                          <div class="form-inline">
                           <%= link_to tipo_integracao, class:"btn btn-default btn-sm" do %>
                              <span class="glyphicon glyphicon-pencil"></span>
                            <% end %>
                          </div>
                        </td>
        <td><%= tipo_integracao.nome %></td>

      </tr>
        <% end %>
      </tbody>
    </table>

              </div>
              <div class="panel-footer">
                <div class="row">
                  <div class="col col-xs-6">
                    <%= page_entries_info @tipo_integracoes %>
                  </div>
                  <div class="col col-xs-6">
                    <ul class="pagination hidden-xs pull-right">
                      <li><%= will_paginate @tipo_integracoes, :previous_label => image_tag('previous.png', :alt => 'Anterior', :class => 'glyphicon glyphicon-chevron-left') ,:next_label => image_tag('next.png', :alt => 'Próximo', :class => 'btn_img') , :separator => ' | ', :params => params %> </li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
